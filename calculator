#include<iostream>
using namespace std;
int main()
{
  int a,b;
  char op;
  cout<<"press \n+ for addition\n- for subtraction\n*for mutiplication\n/ for qoutient\n ";
  cin>>op;
  cout<<"enter the values";  
  cin>>a>>b;
switch(op)
{
  case '+':
  cout<<"sum="<< a+b;
  break;
  case '-':
  cout<<"difference="<< a-b;
  break;
  case '*':
  cout<<"product="<< a*b;
  break;
  case '/':
  cout<<"qoutient="<< a/b;
  break;
} 
return 0;
}
